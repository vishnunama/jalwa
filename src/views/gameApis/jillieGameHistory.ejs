            display: flex;
        }

        .header-row div,
        .data-row div {
            min-width: 110px;
            text-align: center;
            padding: 10px;
            border-bottom: 1px solid #61a9ff;
            background-color: #61a9ff;
            color: #ddd;
        }

        .data-row div {
            border-bottom: 1px solid #ddd;
            background-color: transparent;
            color: #001C54;
        }

        .scroll-container {
            overflow-x: auto;
            max-height: 400px;
            scrollbar-width: none;
            /* Hide scrollbar for Firefox */
            /* -ms-overflow-style: none; Hide scrollbar for IE/Edge */
            white-space: nowrap;
        }

        .scroll-container::-webkit-scrollbar {
            height: 3px;
            /* Change height for horizontal scrollbar */
        }

        .scroll-container::-webkit-scrollbar-thumb {
            background: #61a9ff;
            /* Scrollbar thumb color */
            border-radius: 10px;
            /* Rounded corners for scrollbar thumb */
        }

        .scroll-container::-webkit-scrollbar-thumb:hover {
            background: #555;
            /* Scrollbar thumb color on hover */
        }

        .scroll-container::-webkit-scrollbar-track {
            background: #001C54;
            max-height: 400px;
            /* Scrollbar track color */
        }
    </style>
