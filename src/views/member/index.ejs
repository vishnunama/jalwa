        <script>
            function showPopupActivity() {
                var popupActivity = document.getElementById("welcomePopupActivity");
                var overlay2 = document.getElementById("overlay2");
                // var overlayActivity = document.getElementById("overlay");

                if (popupActivity && overlay2) {
                    popupActivity.style.display = "block";
                    overlay2.style.display = "block";
                    overlayActivity.style.display = "block";
                }
            }

            function updateReminders() {
                var remindersCheckbox = document.getElementById("checkbox-18");
                if (remindersCheckbox.checked) {
                    localStorage.setItem("reminders", "1"); // Set to 1 if checked
                } else {
                    localStorage.setItem("reminders", "0"); // Set to 0 if not checked
                }
            }


            function closePopupActivity() {

                var popupActivity = document.getElementById("welcomePopupActivity");
                var overlayActivity = document.getElementById("overlay");
                var overlay2 = document.getElementById("overlay2");

                if (popupActivity && overlay2) {
                    popupActivity.style.display = "none";
                    overlay2.style.display = "none";
                }
            }

            // Check if the welcome message has been shown before (using local storage)
            window.addEventListener("DOMContentLoaded", (event) => {
                var reminders = parseInt(localStorage.getItem("reminders"));
                var isLogin = parseInt(localStorage.getItem("isLogin"));

                if (reminders == 1) {
                    closePopupActivity();
                } else {
                    showPopupActivity(); // Show the welcome popup only if the user hasn't visited before
                }
            });
        </script>
