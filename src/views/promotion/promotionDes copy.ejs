  <script>

    function formatTimestamp(timestamp) {
      // Create a new Date object from the timestamp
      const date = new Date(timestamp);

      // Extract the components of the date
      const year = date.getFullYear();
      const month = (date.getMonth() + 1).toString().padStart(2, '0');  // months are 0-based, so add 1
      const day = date.getDate().toString().padStart(2, '0');
      const hours = date.getHours().toString().padStart(2, '0');
      const minutes = date.getMinutes().toString().padStart(2, '0');
      const seconds = date.getSeconds().toString().padStart(2, '0');

      // Format and return the string as 'YYYY-MM-DD HH:mm:ss'
      return `${year}-${month}-${day} ${hours}:${minutes}:${seconds}`;
    }
    // Function to fetch subordinates data based on the selected startDate
    const initSubordinatesData = async (startDate) => {
      $(".ar-loading-view").fadeIn();
      try {
        const response = await axios.get(`/api/GetCommissionDetails?startDate=${startDate}`);


        if (response?.data?.status == true) {
          document.getElementById("CommissionDetailsShow").innerHTML =
            `
            <div style="width: 100%; height: auto; padding: 10px; background-color: f0f0f5; border-radius: 10px;">
                <div style="line-height: 22px; margin-bottom: 20px;">
                    <p>Settlement successful</p>
                    <p>${formatTimestamp(startDate)}</p>
                    <p>The commission has been automatically credited to your balance</p>
                </div>

                <div style="display: flex; justify-content: space-between; background-color: white; padding: 10px; border-radius: 6px; margin-bottom: 8px;">
                    <div>
                        <p>Number of bettors</p>
                    </div>
                    <div>
                        <p style="font-weight: 800;" id="subordinatesWithBettingCount">${response?.data?.data?.subordinatesWithBettingCount} People</p>
                    </div>
                </div>
                <div style="display: flex; justify-content: space-between; background-color: white; padding: 10px; border-radius: 6px; margin-bottom: 8px;">
                    <div>
                        <p>Bet amount</p>
                    </div>
                    <div>
                        <p style="font-weight: 800;" id="subordinatesBettingAmount">${response?.data?.data?.subordinatesBettingAmount}</p>
                    </div>
                </div>
                <div style="display: flex; justify-content: space-between; background-color: white; padding: 10px; border-radius: 6px; margin-bottom: 8px;">
                    <div>
                        <p>Commission payout</p>
                    </div>
                    <div>
                        <p style="font-weight: 800;" id="subordinatesWithTotalCommissions">${response?.data?.data?.subordinatesWithTotalCommissions}</p>
                    </div>
                </div>
                <div style="display: flex; justify-content: space-between; background-color: white; padding: 10px; border-radius: 6px; margin-bottom: 8px;">
                    <div>
                        <p>date </p>
                    </div>
                    <div>
                        <p style="font-weight: 800;">${formatTimestamp(startDate)}</p>
                    </div>
                </div>

            </div>
            `
        }


        console.log(response?.data);  // For debugging, logs the full response
      } catch (error) {
        console.log(error);  // Error handling
      }

      $(".ar-loading-view").fadeOut();
    };

    // Event listener to handle date selection
    document.getElementById('selectedDate').addEventListener('change', (event) => {
      const selectedDate = event.target.value;

      // Convert selected date to milliseconds (Unix timestamp)
      const startDate = moment(selectedDate).startOf('day').valueOf();

      // Call the function with the selected startDate
      initSubordinatesData(startDate);
    });

    // Set the default date to yesterday in the input
    const defaultDate = moment().subtract(1, 'days').format('YYYY-MM-DD');
    document.getElementById('selectedDate').value = defaultDate;

    // Initialize data with yesterday's date (default value)
    let startDate = moment(defaultDate).startOf('day').valueOf();
    initSubordinatesData(startDate);

  </script>

